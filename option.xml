<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/6.0/option.xsd">
	<import>
		<options>
			<option name="enable_auto_delete_unconfirmed_users">
				<categoryname>user.register</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<enableoptions>auto_delete_unconfirmed_users_resend_email_days,
					auto_delete_unconfirmed_users_days,
					auto_delete_unconfirmed_users_limit,
					auto_delete_unconfirmed_users_send_email_resent,
					auto_delete_unconfirmed_users_send_email_deleted,
					auto_delete_unconfirmed_users_max_registration_age,
					auto_delete_unconfirmed_users_log</enableoptions>
				<showorder>5</showorder>
			</option>
			<option name="auto_delete_unconfirmed_users_resend_email_days">
				<categoryname>user.register</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>7</defaultvalue>
				<validationpattern>^[0-9]+$</validationpattern>
				<showorder>6</showorder>
				<minvalue>0</minvalue>
				<maxvalue>365</maxvalue>
				<disableAutocomplete>1</disableAutocomplete>
			</option>
			<option name="auto_delete_unconfirmed_users_days">
				<categoryname>user.register</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>7</defaultvalue>
				<validationpattern>^[0-9]+$</validationpattern>
				<showorder>7</showorder>
				<minvalue>1</minvalue>
				<maxvalue>365</maxvalue>
				<disableAutocomplete>1</disableAutocomplete>
			</option>
			<option name="auto_delete_unconfirmed_users_limit">
				<categoryname>user.register</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>10</defaultvalue>
				<showorder>8</showorder>
				<minvalue>1</minvalue>
				<maxvalue>50</maxvalue>
				<disableAutocomplete>1</disableAutocomplete>
			</option>
			<option name="auto_delete_unconfirmed_users_send_email_resent">
				<categoryname>user.register</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<showorder>9</showorder>
			</option>
			<option name="auto_delete_unconfirmed_users_send_email_deleted">
				<categoryname>user.register</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<showorder>10</showorder>
			</option>
			<option name="auto_delete_unconfirmed_users_max_registration_age">
				<categoryname>user.register</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<validationpattern>^[0-9]+$</validationpattern>
				<showorder>11</showorder>
				<minvalue>0</minvalue>
				<maxvalue>730</maxvalue>
				<disableAutocomplete>1</disableAutocomplete>
				<enableoptions>auto_delete_unconfirmed_users_delete_legacy</enableoptions>
			</option>
			<option name="auto_delete_unconfirmed_users_delete_legacy">
				<categoryname>user.register</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>12</showorder>
			</option>
		</options>
	</import>
	<delete>
		<option name="auto_delete_unconfirmed_users_send_email" />
	</delete>
</data>
